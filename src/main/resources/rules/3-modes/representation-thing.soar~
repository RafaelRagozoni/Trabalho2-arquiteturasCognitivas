sp {initialize-food
   (state <s> ^name 3-modes
              ^io.input-link <il>)
-->
   (<il> ^CREATURE <CREATURE>)
   (<CREATURE> ^MEMORY <MEMORY>
               ^PARAMETERS <PARAMETERS>
               ^POSITION <POSITION>
               ^SENSOR <SENSOR>)
   (<PARAMETERS> ^MINFUEL 400
                 ^TIMESTAMP 2)
   (<POSITION> ^X 100
               ^Y 100)
   (<SENSOR> ^FUEL.VALUE 300
             ^VISUAL <V>)
   (<V> ^ENTITY <E>)
   (<E> ^DISTANCE 1
        ^X 1
        ^Y 1
        ^X2 1
        ^Y2 1
        ^TYPE TIPO
        ^NAME NOME
        ^COLOR COR)
}
               